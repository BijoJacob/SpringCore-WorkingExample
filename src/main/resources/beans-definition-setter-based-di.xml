<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">


	<bean id="teamMember" class="com.personal.springcore.bean.TeamMember" scope="singleton">
		<property name="team" ref="team"></property>
	</bean>


	<bean id="team" class="com.personal.springcore.bean.Team" scope="singleton">
		<property name="teamName" value="BACKENDBijo"></property>
	</bean>




</beans>